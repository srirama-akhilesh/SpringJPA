pipeline {
agent any

stages {

stage ('Checkout') {
steps {
    checkout([$class: 'GitSCM',
    branches: [[name: '*/master']], 
    doGenerateSubmoduleConfigurations: false, 
    extensions: [], 
    submoduleCfg: [], 
    userRemoteConfigs: [[url: 'https://github.com/srirama-akhilesh/SpringJPA.git']]])
 }
}
stage ('Compile Stage') {
steps {
withMaven(maven : 'maven3.6.3') {
bat'mvn clean compile'
}
}
}
stage ('Testing Stage') {
steps {
withMaven(maven : 'maven3.6.3') {
bat'mvn test'
}
}
}
stage ('Install Stage') {
steps {
withMaven(maven : 'maven3.6.3') {
bat'mvn install'
}
}
}
}
}
